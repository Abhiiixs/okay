extends base.pug

block scripts
  script(src='/index.js')


  block style  
    style
     include ../static/style.css
     include ../static/stylecontact.css


block content 
  div.conatiner
    h1  Contact us 
    form(action="/contact",method="post",class="form") 
    input(type="text", class="myinput", name ="name",placeholder="Enter your name" required)
    input(type="Phone", class="myinput", name ="phone",placeholder="Enter your Phone No " require)
    input(type="Email", class="myinput", name ="email",placeholder="Enter your Email ID" require)
    input(type="text", class="myinput", name ="text",placeholder="More about you" require)
    button(class="btn" value="submit")
      |submit 

